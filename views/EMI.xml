<?xml version="1.0" encoding="utf-8"?>
<odoo>

        <record id="emi_view_form" model="ir.ui.view">
        <field name="name">IEMI Form</field>
        <field name="model">imei.list</field>
        <field name="arch" type="xml">
        <form>
             <sheet>
                 <!-- <div class="oe_button_box" name="button_box">
